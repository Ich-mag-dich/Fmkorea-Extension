{
    "name": "Fmkorea+",
    "description": "Extension for FmKorea.",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
        "background.js"
        ]
    },
    "permissions": ["storage",
      "activeTab",
      "tabs",
      "notifications",
      "<all_urls>",
      "tabCapture"
    ],
    "browser_action": {
        "default_icon": "./src/img/icon.png"
      },
    "options_page": "options.html",
    "externally_connectable": {
    "matches": ["https://*.google.com/*","https://*.fmkorea.com/*"]
    },
    "content_scripts": [
      {
       "matches": ["https://*.google.com/*","https://*.fmkorea.com/*"],
       "js": ["contentscript.js"]
      }
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}